<?xml version="1.0" encoding="ISO-8859-1"?>
<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
<f:loadBundle basename="message" var="inter" />
<rich:jQuery timing="onload" selector="#grupo" query="focus()" />
<!-- SUGGESTION BOX GRUPO -->
<rich:suggestionbox id="suggestionBoxIdGrupo" for="grupo" tokens=",[]"
    suggestionAction="#{configuracaoBean.autocompleteGrupo}" var="grupo"
    fetchValue="#{grupo}"
    nothingLabel="Nenhum grupo encontrado" columnClasses="center"
    border="1"
    width="300"
    height="100"
    shadowOpacity="4"
    shadowDepth="4"
    cellpadding="1"
    usingSuggestObjects="true">
    <h:column>
        <h:outputText value="#{grupo}" />
    </h:column>
</rich:suggestionbox>
<h:panelGrid columns="2">
	<h:outputText value="*#{inter.grupo}" style="float: right;"/>
	<h:inputText value="#{configuracaoBean.configuracao.grupo}" maxlength="20" style=" width:100px; align:left;" id="grupo"/>
	<h:outputText value="*#{inter.tipo}" style="float: right;"/>
	<h:inputText value="#{configuracaoBean.configuracao.tipo}" maxlength="20" style=" width:100px; align:left;" id="tipo"/>
	<h:outputText value="*#{inter.valor}" style="float: right;"/>
	<h:inputText value="#{configuracaoBean.configuracao.valor}" maxlength="100" style=" width:170px; align:left;" id="valor"/>
	<h:outputText value="#{inter.descricao}" style="float: right;"/>
	<h:inputText value="#{configuracaoBean.configuracao.descricao}" maxlength="100" style=" width:170px; align:left;" id="descricao"/>
</h:panelGrid>
</html>